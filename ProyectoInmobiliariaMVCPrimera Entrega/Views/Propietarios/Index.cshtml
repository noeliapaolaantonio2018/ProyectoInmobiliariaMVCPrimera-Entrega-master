@model IEnumerable<ProyectoInmobiliariaMVCPrimera_Entrega.Models.Propietario>

@{ ViewData["Title"] = "Index"; }

<h1>Propietario</h1>

<p>
    <a asp-action="Create">Crear Propietario</a>
</p>
<table class="table">
    <thead>
        <tr>
            @*<th>
            @Html.DisplayNameFor(model => model.PropietarioId)
        </th>*@
            <th>
                @Html.DisplayNameFor(model => model.Dni)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Apellido)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Telefono)
            </th>

            @*<th>
            @Html.DisplayNameFor(model => model.Clave)
        </th>*@
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            @*<td>
            @Html.DisplayFor(modelItem => item.PropietarioId)
        </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.Dni)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Apellido)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Telefono)
            </td>
            
            @*<td>
            @Html.DisplayFor(modelItem => item.Clave)
        </td>*@
            <td>
                @Html.ActionLink("Editar", "Edit", new { id = item.PropietarioId }) |
                @Html.ActionLink("Eliminar", "Delete", new { id = item.PropietarioId })
            </td>
        </tr>}
    </tbody>
</table>